<% if(typeof errors != 'undefined') { %> 
  <% errors.forEach(function(error) { %>
    <div class="error">
      <%= error.msg %>
    </div>
    <% }); %> 
<% } %>

<% if(success_msg != '') { %>
  <div class="success_msg">
    <%= success_msg %>
  </div>
<% } %> 
  
<% if(error_msg != '') { %>
  <div class="error_msg">
    <%= error_msg %>
  </div>
<% } %>